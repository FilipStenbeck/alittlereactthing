{
  "name": "alittlereactthing",
  "version": "1.0.0",
  "description": "A tiny little start boilerplate for a react/redux app with webpack",
  "main": "main.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run prod || npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --progress --colors --content-base public --history-api-fallback",
    "build": "webpack",
    "prod": "webpack && node server.js"
  },
  "author": "Filip Stenbeck",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "if-env": "^1.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "redux-devtools": "^3.3.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.12"
  }
}
